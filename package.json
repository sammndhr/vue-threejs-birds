{
  "name": "vue-threejs-birds",
  "version": "0.6.9",
  "description": "Threejs birds flocking animation refactored and converted to vue module.",
  "author": {
    "name": "Mrinalini 'Sam' Manandhar",
    "email": "sam@mrinalini.dev"
  },
  "dependencies": {
    "three": "^0.125.0"
  },
  "devDependencies": {
    "@rollup/plugin-buble": "^0.21.1",
    "@rollup/plugin-commonjs": "^11.0.2",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-vue": "^5.1.6",
    "vue-template-compiler": "^2.6.11"
  },
  "keywords": [
    "vue-threejs",
    "vue-threejs-birds",
    "vue",
    "vuejs",
    "animation",
    "3d",
    "background",
    "threejs",
    "vue-plugin",
    "vue-component"
  ],
  "license": "MIT",
  "main": "dist/vue-threejs-birds.umd.js",
  "module": "dist/vue-threejs-birds.esm.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/mrinalini-m/vue-threejs-birds"
  },
  "homepage": "https://github.com/mrinalini-m/vue-threejs-birds#readme",
  "bugs": {
    "url": "https://github.com/mrinalini-m/vue-threejs-birds/issues"
  },
  "scripts": {
    "prepare": "npm run build",
    "build": "npm run build:umd & npm run build:es & npm run build:unpkg",
    "build:umd": "rollup --config build/rollup.config.js --format umd --file dist/vue-threejs-birds.umd.js",
    "build:es": "rollup --config build/rollup.config.js --format es --file dist/vue-threejs-birds.esm.js",
    "build:unpkg": "rollup --config build/rollup.config.js --format iife --file dist/vue-threejs-birds.min.js"
  },
  "unpkg": "dist/vue-threejs-birds.min.js"
}
